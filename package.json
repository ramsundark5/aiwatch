{
  "name": "aiwatch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify && react-native link",
    "start": "react-native start",
    "test": "jest",
    "release": "cd android && ./gradlew clean bundleRelease",
    "run-release": "react-native run-android --variant=release",
    "build:android-debug": "detox build --configuration android.emu.debug",
    "build:android-release": "detox build --configuration android.emu.release",
    "test:android-debug": "detox test --configuration android.emu.debug -l verbose",
    "test:jest-circus:android-debug": "detox test --configuration android.emu.debug -l verbose -o e2e/config-circus.json",
    "test:android-release": "detox test --configuration android.emu.release -l verbose",
    "test:jest-circus:android-release": "detox test --configuration android.emu.release -l verbose -o e2e/config-circus.json",
    "lint": "eslint .",
    "jetify": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.1.1",
    "bugsnag-react-native": "^2.22.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-native": "0.60.3",
    "react-native-admob": "git+https://github.com/ramsundark5/react-native-admob",
    "react-native-app-auth": "^4.4.0",
    "react-native-calendars": "^1.198.0",
    "react-native-config": "^0.11.7",
    "react-native-device-log": "git+https://github.com/ramsundark5/react-native-device-log",
    "react-native-gesture-handler": "1.3.0",
    "react-native-google-signin": "git+https://github.com/ramsundark5/react-native-google-signin",
    "react-native-iap": "3.4.6",
    "react-native-image-overlay": "^0.1.2",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-modal": "^11.1.0",
    "react-native-orientation": "^3.1.3",
    "react-native-paper": "3.0.0-alpha.3",
    "react-native-progress": "git+https://github.com/ramsundark5/react-native-progress",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-slider": "^0.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.6.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-yz-vlcplayer": "ramsundark5/react-native-yz-vlcplayer#master",
    "react-navigation": "^3.11.1",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "react-redux": "^7.1.0",
    "redux-logger": "^3.0.6",
    "redux-starter-kit": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "detox": "^14.3.0",
    "eslint": "^6.3.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.x0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox" : {
    "test-runner": "jest",
    "configurations": {
        "android.emu.debug": {
            "binaryPath": "android/app/build/outputs/apk/debug/app-x86-debug.apk",
            "testBinaryPath": "android/app/build/outputs/apk/androidTest/debug/app-debug-androidTest.apk",
            "build":
            "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
            "type": "android.emulator",
            "name": "Nexus_5X_API_28"
        },
        "android.emu.release": {
            "binaryPath": "android/app/build/outputs/apk/release/app-x86_64-release.apk",
            "build":
            "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
            "type": "android.emulator",
            "name": "Nexus_5X_API_28"
        }
    }
  }
}
